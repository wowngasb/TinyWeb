{
  "name": "TinyWeb/TinyWeb",
  "type": "project",
  "description": "A Simple PHP Framework based on Composer.",
  "keywords": [
    "tiny",
    "web",
    "framework"
  ],
  "homepage": "http://github.com/wowngasb/TinyWeb",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/wowngasb/TinyWeb/issues"
  },
  "authors": [
    {
      "name": "kong lin",
      "email": "konglin4216@gmail.com",
      "homepage": "http://github.com/wowngasb"
    }
  ],
  "require": {
    "php": ">=5.5",
    "illuminate/database": "5.2.*",
    "webonyx/graphql-php": "^0.9",
    "phpFastCache/phpFastCache": "^5.0",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "php-console/php-console": "^3.1",
    "phpunit/phpunit": "4.8.*",
    "php-mock/php-mock-phpunit": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "TinyWeb\\": "TinyWeb/",
      "app\\": "app/"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  }
}
